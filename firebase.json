{
    "hosting": {
      "public": ".next",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "headers": [
        {
          "source": "**",
          "headers": [
            {
              "key": "Permissions-Policy",
              "value": "clipboard-write=(self)"
            }
          ]
        }
      ]
    }
  }
  